{
    "name": "spatie/dns",
    "description": "A easy to handle class that can get all sort of dns related info for a given domain name",
    "keywords": [
        "spatie",
        "dns"
    ],
    "homepage": "https://github.com/spatie/dns",
    "license": "MIT",
    "authors": [
        {
            "name": "Harish Toshniwal",
            "email": "harish@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/process": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Dns\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
